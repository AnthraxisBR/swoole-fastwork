{
    "name": "gabrielmourao/swoole-fw",
    "require": {
        "vlucas/phpdotenv": "^3.3@dev",
        "guzzlehttp/guzzle": "^6.3@dev",
        "symfony/yaml": "^4.3@dev",
        "webonyx/graphql-php": "dev-master",
        "league/oauth2-server": "dev-master",
        "aws/aws-sdk-php": "^3.0@dev",
        "microsoft/azure-storage-blob": "*",
        "microsoft/azure-storage-table": "*",
        "microsoft/azure-storage-queue": "*",
        "microsoft/azure-storage-file": "*",
        "google/cloud": "dev-master",
        "google/apiclient": "^2.0",
        "filp/whoops": "^2.2@dev",
        "czproject/git-php": "dev-master",
        "nelexa/zip": "dev-master",
        "nette/php-generator": "^3.2@dev",
        "kamermans/guzzle-oauth2-subscriber": "dev-master",
        "doctrine/orm-two": "2.6.1"
    },
    "require-dev" : {
        "tightenco/collect": "^5.8.19",
        "swlib/swpdo": "dev-master"
    },
    "repositories" : [
        {
            "type": "vcs",
            "url": "https://github.com/swlib/swpdo"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AnthraxisBR/orm"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AnthraxisBR/dbal"
        }
    ],
    "authors": [
        {
            "name": "Gabriel Melo",
            "email": "gabrielmouraodemelo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AnthraxisBR\\SwooleFW\\": "src/",
            "App\\": "application/",
            "database\\" : "database/",
            "tests\\" : "tests/"
        },
        "psr-0": {
            "AnthraxisBR\\SwooleFW\\": "src/",
            "App\\": "application/",
            "database\\" : "database/",
            "tests\\" : "tests/"
        }
    }
}
